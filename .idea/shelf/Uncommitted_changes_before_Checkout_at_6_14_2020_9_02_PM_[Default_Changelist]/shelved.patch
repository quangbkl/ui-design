@@ -0,0 +1,10 @@
+import {PixelRatio} from 'react-native';
+
+const scaleValue = PixelRatio.get() / 2;
+
+const scaleWithPixel = (size, limitScale = 1.2) => {
+    const value = scaleValue > limitScale ? limitScale : scaleValue;
+    return size * value;
+};
+
+export default scaleWithPixel;
